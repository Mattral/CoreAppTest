# Typeof: development | testing | staging | production
NODE_ENV=

# Generate any random string. Length >= 64
COOKIE_SECRET=

# API URL
BASE_API_URL=

# Company Code for the core app
ADMIN_COMPANY_CODE=

# Frontend key for the core app
ADMIN_FRONTEND_KEY=

# Without the protocol or www; Used for the creating sub-domains
# E.g (example.com)
# DO NOT USE: (www.example.com) or (https://example.com)
FRONTEND_DOMAIN_NAME=


# AWS Access Key; Generated by user
AWS_ACCESS_KEY=

# AWS Access Key; Generated by user
AWS_SECRET_ACCESS_KEY=

# AWS Region
AWS_REGION=


# EC2 instance host address
EC2_INSTANCE_HOST=

# SSH Port for the instance
EC2_INSTANCE_PORT=

# Username of the Instance
EC2_INSTANCE_USERNAME=

# IP address of the EC2 instance. Best to attach an Elastic IP address to the instance
EC2_INSTANCE_IP_ADDRESS=


# Hosted Zone ID of the `FRONTEND_DOMAIN_NAME` on Route53
AWS_ROUTE53_HOSTED_ZONE_ID=
